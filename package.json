{
  "name": "n8n-nodes-csv-json-htmltable-converter",
  "version": "1.2.3",
  "description": "A comprehensive n8n node for seamless bidirectional conversion, replacement, and styling of HTML tables, CSV, and JSON formats. Features advanced table selection (including Table With Caption, Table Under Heading, Last Table), Style and Replace operations, n8nObject output for direct workflow integration, robust validation, customisable formatting, and improved chaining for ETL, web scraping, and data transformation workflows.",
  "keywords": [
    "n8n",
    "n8n-node",
    "n8n-community-node",
    "table",
    "html",
    "csv",
    "json",
    "converter",
    "style",
    "replace",
    "caption",
    "heading",
    "n8nObject",
    "workflow",
    "data transformation",
    "web scraping"
  ],
  "license": "MIT",
  "author": {
    "name": "Max Soukhomlinov",
    "email": "maxs@intellectit.com.au"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/msoukhomlinov/n8n-nodes-csv-json-htmltable-converter.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "test": "jest"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [],
    "nodes": [
      "dist/nodes/CsvJsonHtmltableConverter/CsvJsonHtmltableConverter.node.js"
    ]
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.35",
    "@types/node": "^18.15.11",
    "@types/papaparse": "^5.3.7",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "@typescript-eslint/utils": "^8.31.0",
    "eslint": "^9.0.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "~1.70.0",
    "prettier": "^2.8.7",
    "typescript": "~5.0.4",
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1"
  },
  "dependencies": {
    "cheerio": "^1.0.0",
    "@minify-html/node": "^0.16.4",
    "json-2-csv": "^5.5.9",
    "papaparse": "^5.4.1"
  }
}
